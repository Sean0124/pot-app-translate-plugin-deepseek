{
  "id": "plugin.com.sean.deepseek",
  "plugin_type": "translate",
  "icon": "deepseek.svg",
  "display": "deepseek-ai",
  "homepage": "https://github.com/Sean0124/pot-app-translate-plugin-deepseek.git",
  "needs": [
    {
      "key": "model",
      "display": "模型选择",
      "type": "select",
      "options": {
        "deepseek-chat": "deepseek-chat",
        "deepseek-reasoner": "deepseek-reasoner"
      },
      "default": "deepseek-chat"
    },
    {
      "key": "apiKey",
      "display": "API密钥",
      "type": "input"
    },
    {
      "key": "promte",
      "display": "提示词",
      "type": "input",
      "default": "你是一个专业的翻译者，请将我提供的文本翻译成中文。"
    },
    {
      "key": "wordCount",
      "display": "根据输入单词数,判断是否进行翻译",
      "type": "input",
      "default": "2"
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}